
Ext.define('GUI.store.Users', {
    extend: 'Ext.data.Store',
    model: 'GUI.model.User',
	   
    autoLoad: true,
    fields: ['id','name','email'],  
    data: [
                {name: 'Ed', email: 'ed@sencha.com'},
                {name: 'Tommy', email: 'tommy@sencha.com'}
    ],

    proxy: {
        type: 'ajax',
	url : '/user'
        reader: {
            type: 'json',
	    root: 'users',
	    successProperty: 'success'
        }
    }
});